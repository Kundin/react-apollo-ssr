{
  "name": "react-apollo-ssr",
  "version": "1.2.2",
  "description": "React Apollo SSR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kundin/react-apollo-ssr.git"
  },
  "keywords": [
    "React",
    "Apollo",
    "SSR"
  ],
  "author": "Yury Kundin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kundin/react-apollo-ssr/issues"
  },
  "homepage": "https://github.com/Kundin/react-apollo-ssr#readme",
  "scripts": {
    "dev": "NODE_ENV=development ts-node src/server/index.ts",
    "prod": "NODE_ENV=production node build/server/index.js",
    "compile": "tsc",
    "build": "NODE_ENV=production webpack --config src/builder/index.ts --progress",
    "build:browser": "NODE_ENV=production webpack --config --config src/builder/configBrowser.ts --progress",
    "build:node": "NODE_ENV=production webpack --config src/builder/configNode.ts --progress",
    "analyzer:browser": "NODE_ENV=production webpack --config src/builder/configBrowser.ts --profile --json > stats.json && webpack-bundle-analyzer ./stats.json ./dist/web/",
    "analyzer:node": "NODE_ENV=production webpack --config src/builder/configNode.ts --profile --json > stats.json && webpack-bundle-analyzer ./stats.json ./dist/node/",
    "prettier": "prettier --config './.prettierrc' --write './src/**/*.{js,jsx,ts,tsx,css}'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint './src' --ext '.js, .jsx, .ts, .tsx'",
    "lint:css": "stylelint './src/client/**/*.css'"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@babel/preset-typescript": "^7.12.1",
    "@loadable/component": "5.12.0",
    "@loadable/server": "5.12.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-device": "^0.4.2",
    "file-loader": "^6.2.0",
    "graphql": "^14.7.0",
    "ignore-styles": "^5.0.1",
    "node-fetch": "2.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet-async": "^1.0.7",
    "react-router": "5.1.2",
    "react-router-dom": "5.2.0",
    "terser-webpack-plugin": "^2.3.8",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@loadable/babel-plugin": "5.12.0",
    "@loadable/webpack-plugin": "^5.14.0",
    "@types/react": "^16.9.56",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-preset-airbnb": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "2.0.3",
    "null-loader": "^4.0.1",
    "prettier": "^2.1.2",
    "style-loader": "^1.3.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "defaults"
  ]
}
