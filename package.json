  },
  "homepage": "https://github.com/Kundin/react-apollo-ssr#readme",
  "scripts": {
    "dev": "cross-env BABEL_ENV=node NODE_ENV=development nodemon src/server/bootstrap.js",
    "prod": "cross-env BABEL_ENV=production NODE_ENV=production node lib/server/index.js",
    "build": "cross-env NODE_ENV=production npm run build:webpack && npm run build:lib",
    "build:webpack": "webpack",
    "build:lib": "babel -d lib src",
    "prettier": "prettier --config './.prettierrc' --write './src/**/*.{js,jsx,css}'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint ./src",
    "lint:css": "stylelint './src/client/**/*.css'"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.37",
