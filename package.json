{
  "name": "react-apollo-ssr",
  "version": "1.2.2",
  "description": "React Apollo SSR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kundin/react-apollo-ssr.git"
  },
  "keywords": [
    "React",
    "Apollo",
    "SSR"
  ],
  "author": "Yury Kundin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kundin/react-apollo-ssr/issues"
  },
  "homepage": "https://github.com/Kundin/react-apollo-ssr#readme",
  "scripts": {
    "dev": "NODE_ENV=development ts-node src/server/index.ts",
    "prod": "NODE_ENV=production node build/server/index.js",
    "compile": "tsc",
    "build": "NODE_ENV=production webpack --config src/builder/index.ts --progress",
    "build:browser": "NODE_ENV=production webpack --config --config src/builder/configBrowser.ts --progress",
    "build:node": "NODE_ENV=production webpack --config src/builder/configNode.ts --progress",
    "analyzer:browser": "NODE_ENV=production webpack --config src/builder/configBrowser.ts --profile --json > stats.json && webpack-bundle-analyzer ./stats.json ./dist/web/",
    "analyzer:node": "NODE_ENV=production webpack --config src/builder/configNode.ts --profile --json > stats.json && webpack-bundle-analyzer ./stats.json ./dist/node/",
    "prettier": "prettier --config './.prettierrc' --write './src/**/*.{js,jsx,ts,tsx,css}'",
    "prepare": "is-ci || husky install",
    "lint:all": "npm run lint:js && npm run lint:css",
    "lint:all:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint './' --ext '.js, .jsx, .ts, .tsx'",
    "lint:js:fix": "eslint './' --ext '.js, .jsx, .ts, .tsx' --fix",
    "lint:css": "stylelint **.css",
    "lint:css:fix": "stylelint **.css --fix",
    "code-style:check": "prettier --check .",
    "code-style:fix": "prettier --write .",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@loadable/component": "^5.14.1",
    "@loadable/server": "^5.14.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "graphql": "^14.7.0",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.7",
    "react-router": "^5.2.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.2",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/css-minimizer-webpack-plugin": "^3.0.0",
    "@types/dotenv-webpack": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/loadable__webpack-plugin": "^5.7.1",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/postcss-preset-env": "^6.7.2",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-env": "^1.15.3",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@types/webpack-merge": "^5.0.0",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "dotenv-webpack": "^5.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.6",
    "null-loader": "^4.0.1",
    "postcss": "^8.3.0",
    "postcss-loader": "^4.3.0",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "terser-webpack-plugin": "^5.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.2",
    "typescript-plugin-css-modules": "^2.8.0",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
