    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
